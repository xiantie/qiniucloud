{
  "name": "coud-doc",
  "version": "0.1.0",
  "main": "main.js",
  "private": true,
  "author": {
    "name": "铁咸鱼",
    "email": "303291556@qqcom"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "@electron/remote": "^2.0.1",
    "electron-store": "^8.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildMain": "webpack",
    "onlyEle": "nodemon --watch main.js --watch src/menuTemplate.js --exec \"electron .\"",
    "dev": "concurrently \"wait-on http://localhost:3000 && electron .\" \"cross-env BROWSER=none npm start\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "GITHUB_TOKEN=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 electron-builder",
    "prerelease": "npm run build && npm run buildMain",
    "prepack": "npm run build && npm run buildMain",
    "predist": "npm run build && npm run buildMain"
  },
  "repository": "https://github.com/xiantie/qiniucloud.git",
  "homepage": "./",
  "build": {
    "appId": "cloudDoc",
    "productName": "qiniucloud",
    "copyright": "Copyright@2022 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "settings/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraMetadata": {
      "main": "./build/main.js"
    },
    "publish": ["github"],
    "extends": null,
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}",
      "publisherName": "iron salt fish"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "classnames": "^2.3.1",
    "concurrently": "^6.4.0",
    "electron-is-dev": "^2.0.0",
    "axios": "^0.24.0",
    "qiniu": "^7.4.0",
    "uuid": "^8.3.2",
    "cross-env": "^7.0.3",
    "electron-builder": "^22.14.5",
    "node-sass": "5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-simplemde-editor": "^4.1.5",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.0.1",
    "webpack-cli": "^4.9.1"
  }
}
